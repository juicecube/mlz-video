{
  "name": "mlz-video",
  "version": "1.0.0",
  "description": "",
  "keywords": [],
  "pkgName": "--libraryname--",
  "main": "dist/lib/--libraryname--.js",
  "unpkg": "dist/umd/--libraryname--.js",
  "module": "dist/es/--libraryname--.js",
  "typings": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "author": "",
  "repository": {
    "type": "git",
    "url": ""
  },
  "license": "MIT",
  "engines": {
    "node": ">=6.0.0"
  },
  "scripts": {
    "start": "rollup -c rollup.config.js -w & npm run example",
    "build": "rollup -c rollup.config.js",
    "example": "mlz-pack serve ./example/index.tsx",
    "prebuild": "rm -rf dist",
    "lint": "eslint src/**/*.ts --fix",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "test:prod": "npm run lint && npm run test -- --no-cache",
    "commit": "git-cz",
    "precommit": "lint-staged",
    "prepush": "npm run test:prod && npm run build",
    "commitmsg": "commitlint -E HUSKY_GIT_PARAMS",
    "release": "standard-version",
    "postinstall": "ts-node init.ts"
  },
  "lint-staged": {
    "linters": {
      "*.{js,ts,tsx}": [
        "npm run lint",
        "git add"
      ]
    }
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "jest": {
    "transform": {
      ".(ts|tsx)": "ts-jest"
    },
    "testEnvironment": "node",
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/**/*.d.ts"
    ]
  },
  "prettier": {
    "semi": false,
    "singleQuote": true
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "devDependencies": {
    "@commitlint/cli": "^13.1.0",
    "@commitlint/config-conventional": "^13.1.0",
    "@mlz/lint": "^1.2.1",
    "@mlz/pack": "^1.1.5",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.4",
    "@rollup/plugin-replace": "^2.4.2",
    "@types/inquirer": "^7.3.3",
    "@types/jest": "^26.0.24",
    "@types/node": "^16.4.6",
    "@types/react-dom": "^17.0.9",
    "colors": "^1.4.0",
    "commitizen": "^4.2.2",
    "cross-env": "^7.0.3",
    "cz-conventional-changelog": "^3.3.0",
    "husky": "^7.0.1",
    "inquirer": "^8.1.2",
    "jest": "^27.0.6",
    "jest-config": "^27.0.6",
    "lint-staged": "^10.5.4",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "replace-in-file": "^6.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.55.0",
    "rollup-plugin-terser": "^5.3.1",
    "rollup-plugin-typescript2": "^0.30.0",
    "shelljs": "^0.8.4",
    "standard-version": "^9.3.1",
    "ts-jest": "^27.0.4",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@frontend/yuumi": "^0.3.8",
    "classnames": "^2.3.1",
    "eslint": "^7.31.0",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-scss": "^3.0.0"
  }
}
